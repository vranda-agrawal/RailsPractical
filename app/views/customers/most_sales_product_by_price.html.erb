<h3>most_sales_product_by_price</h3>
<div class="section" >
  <div class="container">
    <table border='2px'>
      <tr>
        <th>First name</th>
        <th>Last name</th>
        <th>Email</th>
        <th>Phone Number</th>
        <th>Total price</th>
      </tr>
      <% @price_list.each do |p| %>
        <% Customer.find(p.customer_id) do |customer| %>
          <tr>
            <td><%= customer.fname %></td>
            <td><%= customer.lname %></td>
            <td><%= customer.email %></td>
            <td><%= customer.phone_number %></td>
            <td><%= p.Sum_of_Total_Price %></td>
          </tr>
        <% end %>
      <% end %>
    </table>
  </div>
</div>
