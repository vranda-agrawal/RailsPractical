<%= form_with model: @product do |form| %>
  <%= form.text_field :name, placeholder: 'Food Product Name' %><br/><br/>
  <%= form.text_field :description, placeholder: 'Food Product Description' %><br/><br/>
  <%= form.number_field :price, placeholder: 'price' %><br/><br/>
  <%= collection_select(:food_product, :my_user_id, MyUser.all, :id, :email) %><br/><br/>
  <%= form.submit %><br/><br/>
<% end %>