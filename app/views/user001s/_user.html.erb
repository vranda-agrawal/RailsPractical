<%= form_for user001 do |form| %>
  <div>
      <%= form.label :name, style: "display: block" %>
      <%= form.text_field :name %>
  </div>
  <div>
      <%= form.label :email, style: "display: block" %>
      <%= form.text_field :email %>
  </div>
  <div>
      <%= form.label :picture, style: "display: block" %>
      <%= form.file_field :picture %>
  </div>
  <div>
      <%= form.submit %>
  </div>

<%end%>
